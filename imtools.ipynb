    "import os\n",
    "\n",
    "def get_imlist(path):\n",
    "    \"\"\"Возвращает списоок имен всех jpg-файлов в каталоге. \"\"\"\n",
    "    \n",
    "    return [os.path.join(path, f) for f in os.listdir(path)\n",
    "                              if f.endswith('.jpg')]"
   ]
